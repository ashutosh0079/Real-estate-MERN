{
    "version": 2,
    "name": "node-js",
    "builds": [
        { 
            "src": "index.js", 
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
          "prisma/schema.prisma",
          "prisma/migrations/**",
          "node_modules/.prisma/client/**"
        ]
      }
   }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/" }
    ]
}
